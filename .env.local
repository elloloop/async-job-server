# Environment for local development with Docker Compose
ASYNC_JOBS_DB_DSN=postgresql://asyncjobs:asyncjobs@localhost:5432/asyncjobs

# LocalStack SQS queues
ASYNC_JOBS_SQS_QUEUE_NOTIFICATIONS=http://localhost:4566/000000000000/notifications
ASYNC_JOBS_SQS_QUEUE_MESSAGE_LABELING=http://localhost:4566/000000000000/message-labeling

# Use case configurations
ASYNC_JOBS_NOTIFICATIONS_MAX_CONCURRENT=10
ASYNC_JOBS_NOTIFICATIONS_DEFAULT_DELAY_TOLERANCE_SECONDS=60
ASYNC_JOBS_MESSAGE_LABELING_MAX_CONCURRENT=10
ASYNC_JOBS_MESSAGE_LABELING_DEFAULT_DELAY_TOLERANCE_SECONDS=60

# Per-tenant quotas
ASYNC_JOBS_PER_TENANT_QUOTAS={"tenant-123": {"max_pending_jobs": 100}}

# AWS LocalStack settings
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_DEFAULT_REGION=us-east-1
AWS_ENDPOINT_URL=http://localhost:4566
